<div class="alignleft">
  <h1><a href="<%- config.url %>" title="<%- config.title %>"><%= config.title %></a></h1>
  <% if (config.subtitle){ %><h2><a href="<%- config.url %>" title="<%- config.subtitle %>"><%= config.subtitle %></a></h2><% } %>
</div>
<nav id="main-nav" class="alignright">
  <ul>
    <% for (var i in theme.menu){ %>
      <li><a href="<%- theme.menu[i] %>"><%= i %></a></li>
    <% } %>
  </ul>
  <div class="clearfix"></div>
</nav>
<div class="clearfix"></div>